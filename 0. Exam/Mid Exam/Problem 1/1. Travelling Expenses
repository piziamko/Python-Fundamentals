vacation_days = int(input())
budget = float(input())
people_count = int(input())
price_fuel_per_km = float(input())
food_expenses_per_person = float(input())
room_price_per_person = float(input())

food_expenses_all = food_expenses_per_person * vacation_days * people_count
hotel_expenses = vacation_days * people_count * room_price_per_person

if people_count > 10:
    hotel_expenses = hotel_expenses * 0.75
else:
    hotel_expenses = hotel_expenses
total = food_expenses_all + hotel_expenses

while budget > total:
    for day in range(1, vacation_days + 1):
        kilometers = int(input())
        total = total + kilometers * price_fuel_per_km
        if day % 3 == 0:
            total = total * 1.4
        if day % 5 == 0:
            total = total * 1.4
        if day % 7 == 0:
            total = total - total / people_count
    break
    
if budget < total:
    print(f"Not enough money to continue the trip. You need {abs(total - budget):.2f}$ more.")
else:
    print(f"You have reached the destination. You have {abs(total - budget):.2f}$ budget left.")
