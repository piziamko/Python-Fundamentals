# Ines

all_palindromes = [word for word in input().split() if word == word[::-1]]
searched_palindrome = input()

print(all_palindromes)
print(f'Found palindrome {all_palindromes.count(searched_palindrome)} times')

#
text = input().split()
palindrome_search = input()

palindromes = []

for word in text:
    if word == word[::-1]:
        palindromes.append(word)

print(palindromes)
print(f"Found palindrome {palindromes.count(palindrome_search)} times")
